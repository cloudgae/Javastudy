package chap04;
/*
 *  *  단들을 먼저 따로 출력
 *  i x j
 *  i 2부터 9까지
 *  j 2부터 9까지
 *  j가 2인동안 i가 2-9
 *  
 */
public class Exam11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for(int i = 2; i < 10; i++) {
			System.out.print(i + "단\t");
		}
		System.out.println();
		for(int j = 2; j < 10; j++) {
			for(int i = 2; i < 10; i++) {
				System.out.print(i + "*" + j + "=" + i*j + "\t");
			}
			System.out.println();
		}
	}

}
