package chap04;
/*
 * break    : switch, 반복문 밖으로 제어를 이동-종료시키는 것
 * continue : 반복문의 처음으로 제어를 이동
 */
public class LoopEx5 {

	public static void main(String[] args) {
		for(int i=2;i<10;i++) {
			System.out.println("\n" + i + "단");
			for(int j=2;j<10;j++) {
//				if(j==5) break; //break가 속한 반복문 종료
				if(j==5) continue;
				System.out.println(i+ "*" + j + "=" + i*j);
			}
		}//졸리당 심심 오ㅑ야 오 맞어 다시 모든 단의 4항ㄱ가지만 출력하는것이에요
	}

}
